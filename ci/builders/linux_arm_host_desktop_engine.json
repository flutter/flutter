{
    "_comment": [
        "Builds for ARM64 Linux Desktop (GNOME/KDE environments).",
        "These builds include fontconfig support for proper font rendering.",
        "Separate from embedded/IoT builds which don't require desktop dependencies."
    ],
    "luci_flags": {
      "upload_content_hash": true
    },
    "builds": [
        {
            "archives": [
                {
                    "name": "ci/linux_profile_arm64_desktop",
                    "type": "gcs",
                    "base_path": "out/ci/linux_profile_arm64_desktop/zip_archives/",
                    "include_paths": [
                        "out/ci/linux_profile_arm64_desktop/zip_archives/linux-arm64-profile/linux-arm64-flutter-gtk.zip"
                    ],
                    "realm": "production"
                }
            ],
            "drone_dimensions": [
                "device_type=none",
                "os=Linux"
            ],
            "gclient_variables": {
                "download_android_deps": false,
                "download_jdk": false,
                "use_rbe": true
            },
            "gn": [
                "--target-dir",
                "ci/linux_profile_arm64_desktop",
                "--runtime-mode",
                "profile",
                "--no-lto",
                "--target-os=linux",
                "--linux-cpu=arm64",
                "--prebuilt-dart-sdk",
                "--rbe",
                "--no-goma",
                "--enable-fontconfig"
            ],
            "name": "ci/linux_profile_arm64_desktop",
            "description": "Produces profile mode Flutter GTK artifacts for ARM64 Linux desktop with fontconfig support.",
            "ninja": {
                "config": "ci/linux_profile_arm64_desktop",
                "targets": [
                    "flutter/shell/platform/linux:flutter_gtk"
                ]
            }
        },
        {
            "archives": [
                {
                    "name": "ci/linux_debug_arm64_desktop",
                    "type": "gcs",
                    "base_path": "out/ci/linux_debug_arm64_desktop/zip_archives/",
                    "include_paths": [
                        "out/ci/linux_debug_arm64_desktop/zip_archives/linux-arm64-debug/linux-arm64-flutter-gtk.zip"
                    ],
                    "realm": "production"
                }
            ],
            "drone_dimensions": [
                "device_type=none",
                "os=Linux"
            ],
            "gclient_variables": {
                "download_android_deps": false,
                "download_jdk": false,
                "use_rbe": true
            },
            "gn": [
                "--target-dir",
                "ci/linux_debug_arm64_desktop",
                "--runtime-mode",
                "debug",
                "--target-os=linux",
                "--linux-cpu=arm64",
                "--prebuilt-dart-sdk",
                "--rbe",
                "--no-goma",
                "--enable-fontconfig"
            ],
            "name": "ci/linux_debug_arm64_desktop",
            "description": "Produces debug mode Flutter GTK artifacts for ARM64 Linux desktop with fontconfig support.",
            "ninja": {
                "config": "ci/linux_debug_arm64_desktop",
                "targets": [
                    "flutter/shell/platform/linux:flutter_gtk"
                ]
            }
        },
        {
            "archives": [
                {
                    "name": "ci/linux_release_arm64_desktop",
                    "type": "gcs",
                    "base_path": "out/ci/linux_release_arm64_desktop/zip_archives/",
                    "include_paths": [
                        "out/ci/linux_release_arm64_desktop/zip_archives/linux-arm64-release/linux-arm64-flutter-gtk.zip"
                    ],
                    "realm": "production"
                }
            ],
            "drone_dimensions": [
                "device_type=none",
                "os=Linux"
            ],
            "gclient_variables": {
                "download_android_deps": false,
                "download_jdk": false,
                "use_rbe": true
            },
            "gn": [
                "--target-dir",
                "ci/linux_release_arm64_desktop",
                "--runtime-mode",
                "release",
                "--target-os=linux",
                "--linux-cpu=arm64",
                "--prebuilt-dart-sdk",
                "--rbe",
                "--no-goma",
                "--enable-fontconfig"
            ],
            "name": "ci/linux_release_arm64_desktop",
            "description": "Produces release mode Flutter GTK artifacts for ARM64 Linux desktop with fontconfig support.",
            "ninja": {
                "config": "ci/linux_release_arm64_desktop",
                "targets": [
                    "flutter/shell/platform/linux:flutter_gtk"
                ]
            }
        }
    ]
}

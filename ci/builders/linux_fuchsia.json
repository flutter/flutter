        {
            "drone_dimensions": [
                "device_type=none",
                "os=Linux"
            ],
            "gclient_variables": {
                "download_android_deps": false
            },
            "gn": [
                "--fuchsia",
                "--fuchsia-cpu",
                "x64",
                "--runtime-mode",
                "debug",
                "--no-lto"
            ],
            "name": "fuchsia_debug_x64",
            "ninja": {

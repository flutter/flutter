            ],
            "name": "fuchsia_profile_x64",
            "ninja": {
                "config": "fuchsia_profile_x64",
                "targets": [
                    "flutter/shell/platform/fuchsia:fuchsia"
                ]
            }
        },
        {
            "drone_dimensions": [
                "device_type=none",

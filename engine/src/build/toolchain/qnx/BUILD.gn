import("//build/toolchain/gcc_toolchain.gni")

gcc_toolchain("qnx") {
  asm = "qcc"
  cc = "qcc"
  cxx = "q++"

  readelf = "readelf"
  nm = "ntoaarch64-nm"
  ar = "ntoaarch64-ar"
  ld = "q++"
  strip = "ntoaarch64-strip"

  toolchain_cpu = "arm64"
  toolchain_os = "linux"
  is_clang = false
}

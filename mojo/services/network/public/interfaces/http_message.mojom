module mojo;

import "mojo/public/interfaces/network/http_header.mojom";

struct HttpRequest {
  string method = "GET";
  string url;
  array<HttpHeader>? headers;
  handle<data_pipe_consumer>? body;
};

struct HttpResponse {
  uint32 status_code = 200;
  array<HttpHeader>? headers;
  handle<data_pipe_consumer>? body;
};

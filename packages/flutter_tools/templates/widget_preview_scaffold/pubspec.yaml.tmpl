name: widget_preview_scaffold
description: Scaffolding for Flutter Widget Previews
publish_to: 'none'
version: 0.0.1

environment:
  sdk: {{dartSdkVersionBounds}}

dependencies:
  flutter:
    sdk: flutter
  flutter_test:
    sdk: flutter
  # These will be replaced with proper constraints after the template is hydrated.
